"syllabi": {
    "intsem": "COURSE: Integration Seminar (WWI-INT)\n\nFOCUS: Bridging Theory and Practice.\nTOPICS: Scientific Writing, Project Mgmt, Presentation Skills.\nEXAM: Paper (70%) + Talk (30%).",
    "webeng": "COURSE: Web Engineering (INF-WEB)\n\nFOCUS: Modern Web Stacks.\nTOPICS: React, Node.js, REST vs GraphQL, MCP Basics.\nEXAM: Project Portfolio.",
    "cloud": "COURSE: Cloud Computing (INF-CLD)\n\nFOCUS: Scalable Infrastructure.\nTOPICS: AWS/Azure Basics, Docker, Kubernetes, Serverless.\nEXAM: Written Exam (90 min).",
    "datasci": "COURSE: Data Science (WWI-DS)\n\nFOCUS: Extracting Knowledge from Data.\nTOPICS: Python Pandas, Scikit-Learn, Neural Networks, Ethics in AI.\nEXAM: Project.",
    "sysarch": "COURSE: System Architectures\n\nFOCUS: Enterprise Patterns.\nTOPICS: Microservices, Event-Driven Architecture, Legacy Migration.\nEXAM: Oral Exam.",
    "itlaw": "COURSE: IT Law\n\nFOCUS: Legal Frameworks.\nTOPICS: GDPR, Copyright, Software Contracts, Liability.\nEXAM: Written Exam.",
    "algo": "COURSE: Algorithms\n\nFOCUS: Efficiency.\nTOPICS: Sorting, Graph Theory, Big-O Notation.\nEXAM: Written Exam.",
    "math": "COURSE: Applied Math\n\nFOCUS: Calculus & Linear Algebra.\nTOPICS: Matrices, Vectors, Differential Equations.\nEXAM: Written Exam.",
    "prog": "COURSE: Programming I\n\nFOCUS: Java/Python Basics.\nTOPICS: Variables, Loops, OOP, Clean Code.\nEXAM: Coding Test."
  },
